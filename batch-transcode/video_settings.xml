<?xml version="1.0" encoding="UTF-8"?>

<video_settings>
    <settings_group name='x264' enabled='0'>
        
    </settings_group>
    <settings_group name='avconv'>
        <setting name='vcodec'>libx264</setting>
        <setting name='preset'>ultrafast</setting> <!-- Ultrafast, superfast, veryfast, faster, fast, medium, slow, slower, veryslow, placebo -->
        <setting name='an' />   <!--    No Audio?   -->
        <setting name='y' />    <!--    Overwrite   -->
        <setting name='sn' />   <!--    No Subs     -->
        <setting name='slow-firstpass' disabled='1' />
        <setting name='tune' >film</setting>    <!--    film, animation, grain, stillimage, psnr, ssim, fastdecode, zerolatency --> 
    </settings_group>
    <settings_group name='fix_dvds' disabled='1' />
    <settings_group name='deinterlace' disabled='1' />
    <settings_group name='br_percent' disabled='1' value='95' />
</video_settings>